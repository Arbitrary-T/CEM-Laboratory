<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cu.controllers.tabs.DatabaseManagementTabViewController">
   <children>
      <AnchorPane HBox.hgrow="ALWAYS">
         <children>
            <TabPane tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <tabs>
                <Tab text="Students">
                  <content>
                        <HBox>
                           <children>
                              <BorderPane HBox.hgrow="ALWAYS">
                                 <center>
                                    <HBox BorderPane.alignment="CENTER">
                                       <children>
                                          <VBox HBox.hgrow="ALWAYS">
                                             <children>
                                                <HBox>
                                                   <children>
                                                      <Label text="Filter:">
                                                         <HBox.margin>
                                                            <Insets left="5.0" right="10.0" top="3.0" />
                                                         </HBox.margin>
                                                         <font>
                                                            <Font size="14.0" />
                                                         </font>
                                                      </Label>
                                                      <TextField prefHeight="25.0" HBox.hgrow="ALWAYS">
                                                         <padding>
                                                            <Insets left="10.0" />
                                                         </padding>
                                                         <HBox.margin>
                                                            <Insets top="2.0" />
                                                         </HBox.margin>
                                                      </TextField>
                                                   </children>
                                                   <VBox.margin>
                                                      <Insets top="5.0" />
                                                   </VBox.margin>
                                                </HBox>
                                                <TableView fx:id="databaseTableView" editable="true" prefHeight="200.0" VBox.vgrow="ALWAYS">
                                                   <columns>
                                                      <TableColumn fx:id="studentIDColumn" prefWidth="75.0" text="Student ID" />
                                                      <TableColumn fx:id="studentNameColumn" prefWidth="75.0" text="Student Name" />
                                                      <TableColumn fx:id="studentCourseColumn" prefWidth="75.0" text="Student Course" />
                                                      <TableColumn fx:id="studentEmailColumn" prefWidth="75.0" text="Student Email" />
                                                      <TableColumn fx:id="studentPhoneNumberColumn" prefWidth="75.0" text="Student Phone Number" />
                                                   </columns>
                                                   <VBox.margin>
                                                      <Insets top="5.0" />
                                                   </VBox.margin>
                                                   <columnResizePolicy>
                                                      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                                   </columnResizePolicy>
                                                </TableView>
                                             </children>
                                          </VBox>
                                       </children>
                                    </HBox>
                                 </center>
                                 <right>
                                    <VBox prefHeight="370.0" prefWidth="175.0" BorderPane.alignment="CENTER" />
                                 </right>
                              </BorderPane>
                           </children>
                        </HBox>
                  </content>
                </Tab>
                <Tab text="Equipment">
                     <content>
                        <HBox>
                           <children>
                              <BorderPane HBox.hgrow="ALWAYS">
                                 <center>
                                    <HBox BorderPane.alignment="CENTER">
                                       <children>
                                          <VBox HBox.hgrow="ALWAYS">
                                             <children>
                                                <HBox>
                                                   <children>
                                                      <Label text="Filter:">
                                                         <HBox.margin>
                                                            <Insets left="5.0" right="10.0" top="3.0" />
                                                         </HBox.margin>
                                                         <font>
                                                            <Font size="14.0" />
                                                         </font>
                                                      </Label>
                                                      <TextField fx:id="filterTextField" prefHeight="25.0" HBox.hgrow="ALWAYS">
                                                         <padding>
                                                            <Insets left="10.0" />
                                                         </padding>
                                                         <HBox.margin>
                                                            <Insets top="2.0" />
                                                         </HBox.margin>
                                                      </TextField>
                                                   </children>
                                                   <VBox.margin>
                                                      <Insets top="5.0" />
                                                   </VBox.margin>
                                                </HBox>
                                                <TableView editable="true" prefHeight="200.0" VBox.vgrow="ALWAYS">
                                                   <columns>
                                                      <TableColumn prefWidth="75.0" text="C1" />
                                                      <TableColumn prefWidth="75.0" text="C2" />
                                                      <TableColumn prefWidth="75.0" text="C3" />
                                                      <TableColumn prefWidth="75.0" text="C4" />
                                                      <TableColumn prefWidth="75.0" text="C5" />
                                                      <TableColumn prefWidth="75.0" text="C6" />
                                                   </columns>
                                                   <VBox.margin>
                                                      <Insets top="5.0" />
                                                   </VBox.margin>
                                                   <columnResizePolicy>
                                                      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                                   </columnResizePolicy>
                                                </TableView>
                                             </children>
                                          </VBox>
                                       </children>
                                    </HBox>
                                 </center>
                                 <right>
                                    <VBox prefHeight="370.0" prefWidth="175.0" BorderPane.alignment="CENTER" />
                                 </right>
                              </BorderPane>
                           </children>
                        </HBox>
                     </content>
                </Tab>
                  <Tab text="Untitled Tab">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children>
      </AnchorPane>
   </children>
</HBox>
