<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import org.controlsfx.control.*?>
<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cu.controllers.tabs.LeaseTabViewController">
   <children>
      <AnchorPane HBox.hgrow="ALWAYS">
         <children>
            <SplitPane fx:id="mainVerticalSplitPane" dividerPositions="0.35" prefHeight="720" prefWidth="1280" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <items>
                  <AnchorPane fx:id="leftAP" minHeight="0.0" minWidth="0.0" SplitPane.resizableWithParent="false">
                     <children>
                        <VBox>
                           <children>
                              <ImageView fx:id="studentCardBack" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../resources/images/studentid.png" />
                                 </image>
                              </ImageView>
                              <PieChart fx:id="pieChart" />
                           </children>
                        </VBox>
                        <Group fx:id="textGroup">
                           <children>
                              <VBox fx:id="labelsVBox" layoutX="10.0" layoutY="-5.0">
                                 <children>
                                    <Label fx:id="stdNameLabel">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                    </Label>
                                    <Label fx:id="stdIDLabel">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                    </Label>
                                    <Label fx:id="stdEmailLabel">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                    </Label>
                                    <Label fx:id="stdCourseLabel">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                    </Label>
                                    <Label fx:id="stdPhoneNumberLabel">
                                       <font>
                                          <Font name="System Bold" size="12.0" />
                                       </font>
                                    </Label>
                                 </children>
                              </VBox>
                           </children>
                        </Group>
                     </children>
                  </AnchorPane>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" SplitPane.resizableWithParent="false">
                     <children>
                        <BorderPane prefHeight="718.0" prefWidth="892.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <center>
                              <TableView fx:id="leasedItemsTableView" prefHeight="195.0" prefWidth="415.0" BorderPane.alignment="CENTER">
                                 <columns>
                                    <TableColumn prefWidth="89.0" text="Item" />
                                    <TableColumn prefWidth="115.0" text="Leased To" />
                                    <TableColumn prefWidth="115.0" text="Leased On" />
                                    <TableColumn prefWidth="115.0" text="Time Left" />
                                    <TableColumn prefWidth="115.0" text="Notes" />
                                 </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </center>
                           <bottom>
                              <AnchorPane fx:id="mainBottomAP" BorderPane.alignment="CENTER">
                                 <children>
                                    <ListSelectionView fx:id="listSelectionView" />
                                 </children>
                              </AnchorPane>
                           </bottom>
                        </BorderPane>
                     </children>
                  </AnchorPane>
               </items>
            </SplitPane>
         </children>
      </AnchorPane>
   </children>
</HBox>
